{
    "configurations": [
        {
            "name": "Launch Over SSH",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/src/dashd",
            "arguments": "-printtoconsole",
            "cwd": "${workspaceRoot}",
            "ssh": {
                "host": "188.166.106.146",
                "cwd": "/root/dash",
                "keyfile": "/root/workspace/credentials/id_rsa",
                "user": "root",
                // "forwardX11": true,
                // "x11host": "localhost",
                // "x11port": 6000
                
            },
            "valuesFormatting": "parseText",
            "preLaunchTask": "",
        },
        {
            "name": "Debugging Daemon",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/unfyd",
            "args": [
                "-printtoconsole"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

        {
            "name": "Debugging Qt",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/qt/unfy-qt",
            "args": [
                "-printtoconsole"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "DISPLAY",
                    "value": "172.30.240.1:0.0",
                },
                {
                    "name": "QT_XKB_CONFIG_ROOT",
                    "value": "/usr/share/X11/xkb",
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },


        
       
    ],
    "version": "2.0.0"
}